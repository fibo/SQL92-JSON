{
  "name": "sql92-json",
  "version": "0.5.0",
  "description": "can stringify a JSON into an SQL and viceversa parse an SQL and serialize it into a JSON",
  "main": "sql92-json.js",
  "scripts": {
    "coverage": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "lint": "standard",
    "postversion": "git push origin v${npm_package_version}; npm publish; git push origin master",
    "test": "NODE_PATH=. tape test/*.js test/*/*js | tap-spec"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fibo/SQL92-JSON.git"
  },
  "keywords": [
    "sql",
    "json"
  ],
  "author": {
    "name": "Gianluca Casati",
    "url": "http://g14n.info"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fibo/SQL92-JSON/issues"
  },
  "homepage": "http://g14n.info/SQL92-JSON",
  "devDependencies": {
    "coveralls": "^2.13.0",
    "dot-editorconfig": "^0.4.0",
    "nyc": "^10.2.0",
    "pre-commit": "^1.2.2",
    "standard": "^9.0.2",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3"
  }
}
