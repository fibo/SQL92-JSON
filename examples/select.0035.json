{
  "SELECT": [
    "t.day",
    "co.isocode",
    "ca.name",
    "t.num"
  ],
  "FROM": [
    {
      "t": "dwh.revenue",
      "JOIN": {
        "ca": "dim.campaign",
        "ON": [ "ca.id", { "=": "t.campaignid" } ],
        "JOIN": {
          "co": "dim.country",
          "ON": [ "co.id", { "=": "t.countryid" } ]
        }
      }
    }
  ]
}
